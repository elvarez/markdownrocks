%h1
  My wikis

%table
  %tbody
    - @wikis.each do |wiki|
      %tr
        %td
          = link_to (markdown wiki.title), controller: "wikis", action: "show", id: wiki
        %td
          = link_to "edit", controller: "wikis", action: "edit", id: wiki
        %td
          = link_to "delete", wiki, method: :delete


%p
  = link_to "New", new_wiki_path
